{
    "name": "corus-framework/corus",
    "type": "library",
    "description": "Fast dependency injection framework based on Symfony components",
    "keywords": [ "micro", "framework", "symfony", "container", "depedency injection" ],
    "homepage": "https://github.com/unwarysheep/corus",
    "require": {
        "symfony/dependency-injection": "^3.0",
        "symfony/event-dispatcher": "^3.0",
        "symfony/routing": "^3.0",
        "symfony/config": "^3.0",
        "symfony/http-foundation": "^3.0",
        "symfony/http-kernel": "^3.0",
        "symfony/proxy-manager-bridge": "^3.0",
        "symfony/yaml": "^3.0",
        "symfony/stopwatch": "^3.0",
        "doctrine/orm": "^2.5",
        "twig/twig": "^1.24",
        "symfony/class-loader": "^3.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.2",
        "phpunit/phpunit": "^5.2",
        "mikey179/vfsStream": "^1.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Deeley",
            "email": "unwarysheep@icloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corus\\Framework\\": "src"
         },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Corus\\Framework\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
